                                                    <then>
                                                        <output expression="&quot;Okay! Input another grade&quot;" newline="True"/>
                                                    </then>
                                                    <else>
                                                        <output expression="&quot;Okay! Thank you.&quot;" newline="True"/>
                                                        <breakpoint/>
                                                    </else>
                                                </if>
                                            </then>
                                            <else>
                                                <if expression="Grade&gt;78">
                                                    <then>
                                                        <output expression="&quot;It's okay. You still have the grade of 2.0&quot;" newline="True"/>
                                                        <output expression="&quot;Would you like to try again? Yes or No?&quot;" newline="True"/>
                                                        <input variable="Answer"/>
                                                        <if expression="Answer == &quot;Yes&quot;">
                                                            <then>
                                                                <output expression="&quot;Okay! Input another grade&quot;" newline="True"/>
                                                            </then>
                                                            <else>
                                                                <output expression="&quot;Okay! Thank you.&quot;" newline="True"/>
                                                                <breakpoint/>
                                                            </else>
                                                        </if>
                                                    </then>
                                                    <else>
                                                        <if expression="Grade&gt;74">
