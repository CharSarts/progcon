                                <if expression="Answer == &quot;Yes&quot;">
                                    <then>
                                        <output expression="&quot;Okay! Input another grade&quot;" newline="True"/>
                                    </then>
                                    <else>
                                        <output expression="&quot;Okay! Thank you.&quot;" newline="True"/>
                                        <breakpoint/>
                                    </else>
                                </if>
                            </then>
                            <else>
                                <if expression="Grade&gt;86">
                                    <then>
                                        <output expression="&quot;Congrats! You have the grade of 3.0&quot;" newline="True"/>
                                        <output expression="&quot;Would you like to try again? Yes or No?&quot;" newline="True"/>
                                        <input variable="Answer"/>
                                        <if expression="Answer == &quot;Yes&quot;">
                                            <then>
                                                <output expression="&quot;Okay! Input another grade&quot;" newline="True"/>
                                            </then>
                                            <else>
                                                <output expression="&quot;Okay! Thank you.&quot;" newline="True"/>
                                                <breakpoint/>
                                            </else>
                                        </if>
                                    </then>
                                    <else>
                                        <if expression="Grade&gt;82">
                                            <then>
                                                <output expression="&quot;Great! You have the grade of 2.5&quot;" newline="True"/>
                                                <output expression="&quot;Would you like to try again? Yes or No?&quot;" newline="True"/>
                                                <input variable="Answer"/>
