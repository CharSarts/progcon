    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Hi! Welcome to this program.&quot;" newline="True"/>
            <output expression="&quot;Please input your grade in order for you to know what is the equivalent grade for that.&quot;" newline="True"/>
            <declare name="Grade" type="Integer" array="False" size=""/>
            <declare name="Again" type="Boolean" array="False" size=""/>
            <assign variable="Again" expression="True"/>
            <declare name="Answer" type="String" array="False" size=""/>
            <while expression="Again == True">
                <input variable="Grade"/>
                <if expression="Grade&gt;94">
                    <then>
                        <output expression="&quot;Amazing! You have the grade of 4.0&quot;" newline="True"/>
                        <output expression="&quot;Would you like to try again? Yes or No?&quot;" newline="True"/>
                        <input variable="Answer"/>
                        <if expression="Answer == &quot;Yes&quot;">
                            <then>
                                <output expression="&quot;Okay! Input another grade&quot;" newline="True"/>
                            </then>
                            <else>
                                <output expression="&quot;Okay! Thank you.&quot;" newline="True"/>
                                <breakpoint/>
                            </else>
                        </if>
                    </then>
                    <else>
                        <if expression="Grade&gt;90">
                            <then>
                                <output expression="&quot;Good job! You have the grade of 3.5&quot;" newline="True"/>
                                <output expression="&quot;Would you like to try again? Yes or No?&quot;" newline="True"/>
